{
    "$schema": "https://turbo.build/schema.json",
    "globalEnv": [
        "JPA_*"
    ],
    "tasks": {
        "dev": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "outputs": ["dist/**", "build/**", ".svelte-kit/**"],
            "dependsOn": ["^build"]
        },
        "format": {
            "cache": false
        }
    }
}
